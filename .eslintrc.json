{
    "extends": ["eslint:recommended", "prettier", "prettier/react","plugin:react/recommended"],
    "plugins":[],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "react/react-in-jsx-scope": "off"
    },
    "globals": {
        "React": "writable"
      }
}